{%liquid
	if shop.metafields.arena.wl_cp_settings.type == blank    
    	assign arn_wl_cp_settings = shop.metafields.arena.wl_cp_settings
    else     
    	assign arn_wl_cp_settings = shop.metafields.arena.wl_cp_settings.value
    endif    
%}
<svg xmlnsXlink="http://www.w3.org/2000/svg" style="display: none;">
    <symbol id="arn_icon-add-wishlist" viewBox="{{ arn_wl_cp_settings.wishlist_settings.add_class_icon.view_box | default: '0 0 0 0' }}">
        <title>add wishlist</title>        
        {{ arn_wl_cp_settings.wishlist_settings.add_class_icon.svg }}
    </symbol>
   <symbol id="arn_icon-add-wishlist-product" viewBox="{{ arn_wl_cp_settings.wishlist_settings.add_class_icon_product.view_box | default: '0 0 0 0' }}">
        <title>add wishlist</title>
        {{ arn_wl_cp_settings.wishlist_settings.add_class_icon_product.svg }}
    </symbol>

    <symbol id="arn_icon-show-wishlist" viewBox="{{ arn_wl_cp_settings.wishlist_settings.show_class_icon.view_box | default: '0 0 0 0' }}">
        <title>show wishlist</title>
        {{ arn_wl_cp_settings.wishlist_settings.show_class_icon.svg }}
    </symbol>

    <symbol id="arn_icon-remove-wishlist" viewBox="{{ arn_wl_cp_settings.wishlist_settings.remove_class_icon.view_box | default: '0 0 0 0' }}"> 
        <title>{{ arn_wl_cp_settings.wishlist_settings.table_delete_heading }}</title>
        {{ arn_wl_cp_settings.wishlist_settings.remove_class_icon.svg }}
    </symbol>

    <symbol id="arn_icon-add-compare" viewBox="{{ arn_wl_cp_settings.compare_settings.add_class_icon.view_box | default: '0 0 0 0' }}">
        <title>add compare</title>
        {{ arn_wl_cp_settings.compare_settings.add_class_icon.svg }}
    </symbol>
    <symbol id="arn_icon-add-compare-product" viewBox="{{ arn_wl_cp_settings.compare_settings.add_class_icon_product.view_box | default: '0 0 0 0' }}">
        <title>add compare</title>
        {{ arn_wl_cp_settings.compare_settings.add_class_icon_product.svg }}
    </symbol>

    <symbol id="arn_icon-show-compare" viewBox="{{ arn_wl_cp_settings.compare_settings.show_class_icon.view_box | default: '0 0 0 0' }}">
        <title>show compare</title>
        {{ arn_wl_cp_settings.compare_settings.show_class_icon.svg }}
    </symbol>

    <symbol id="arn_icon-remove-compare" viewBox="{{ arn_wl_cp_settings.compare_settings.remove_class_icon.view_box | default: '0 0 0 0' }}"> 
        <title>{{ arn_wl_cp_settings.compare_settings.table_delete_heading }}</title>
        {{ arn_wl_cp_settings.compare_settings.remove_class_icon.svg }}
    </symbol>
    
    <symbol id="arn_icon-preloader" viewBox="{{ arn_wl_cp_settings.general_settings.preloader_icon.view_box | default: '0 0 0 0' }}"> 
        <title>preloader</title>
        {{ arn_wl_cp_settings.general_settings.preloader_icon.svg }}
    </symbol>
</svg>