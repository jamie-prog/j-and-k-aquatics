            <script defer crossorigin="anonymous" src="{{ 'arn-wishlist-compare.js'  | asset_url }}"></script>
          {% endif %}

{% render 'hulkapps-reorder-css' %}
    
{% render 'wishlisthero-styles.liquid' %}</head>

{%- capture themeClass -%}template{{ template | split: '.' | first | camelize -}}
{%- endcapture -%}{%- assign themeClass = themeClass | strip_newlines | split: '|' | join: ' ' -%}

<body class="{{ themeClass }}" onclick tabIndex=0>
  

  
  
    {%- render 'fb-open-graph' -%}
    {%- if template == 'index' -%}<link href="{{ 'homepage.min.css' | asset_url }}" rel="stylesheet" type="text/css" media="all" >
    {%- elsif template contains 'collection' -%}<link href="{{ 'collection.min.css' | asset_url }}" rel="stylesheet" type="text/css" media="all">
    {%- elsif template contains 'product' -%}<link href="{{ 'product.min.css' | asset_url }}" rel="stylesheet" type="text/css" media="all">
    {%- elsif template contains 'cart' -%}<link href="{{ 'cart.min.css' | asset_url }}" rel="stylesheet" type="text/css" media="all">
    {%- elsif template contains 'blog' or template contains 'article' -%}<link href="{{ 'blog.min.css' | asset_url }}" rel="stylesheet" type="text/css" media="all">
    {%- else -%}<link href="{{ 'pages.min.css'| asset_url }}" rel="stylesheet" type="text/css" media="all">
    {%- endif -%}

    {%- if settings.currency_type == '2' -%}
      {{- "/services/javascripts/currencies.js" | script_tag -}}
      <script>Loader.require('currencyJS', 'scripts');</script>
    {%- endif -%}

    {%- if request.design_mode -%}
        <link rel="stylesheet" href="{{ 'footer.css' | asset_url }}" as="style">
    {%- endif -%}

    {%- capture growaveSettings -%}{{- settings.wishlist_app -}}{{- settings.review_app -}}{%- endcapture -%}
    {%- if growaveSettings contains 'growave' -%}
        {% capture the_snippet_content %}{% render 'socialshopwave-helper' %}{% endcapture %}
        {% unless the_snippet_content contains 'Liquid error' %}{{ the_snippet_content }}{% endunless %}
    {%- endif -%}

    {%- capture wl_cp_Include -%}
        {%- capture content -%}{% include 'arn_icons_define' %}{%- endcapture -%}
        {%- render 'compress', content: content -%}
    {%- endcapture -%}{%- unless wl_cp_Include contains 'Liquid error' -%}{{ wl_cp_Include }}{%- endunless -%}

    {%- if settings.enable_customCss -%}{%- section 'section-css-custom' -%}{%- else -%}{%- render 'snippet-theme', type: 'themeCSS' -%}{%- endif -%}

    <div class="boxed-wrapper">
        {%- capture header -%}{%- include 'header-layout' -%}{%- endcapture -%}
        {%- assign header = header | split: '<!--split:mm-->' -%}

        {{header[0]}}
        {{header[2]}}


        <main id="main-content" role="main">
          

            {%- if template == 'collection' -%}
              {{ content_for_layout }}
            {% elsif template == 'cart' %}
              {% assign position = 'center' %}
              {% assign font_size = '16px' %}
              {% if customer %}
                {{ content_for_layout }}
              {% else %}
                {% render 'wholesale-price-message', position: position, font_size: font_size %}
              {% endif%}
            {% elsif template contains 'media' %}
              {% assign position = 'center' %}
              {% assign font_size = '16px' %}
              {% render 'wholesale-price-message', position: position, font_size: font_size %}
            {%- else -%}
              {%- include 'breadcrumb', pageLayout: settings.page_layout -%}
              <div class="container-wrapper">{{ content_for_layout }}</div>
            {%- endif -%}



            {%- if settings.enable_iconbox -%}
                {%- if request.design_mode -%}
                    {%- section 'section-iconbox' -%}
                {%- else -%}
                    <div class="lazyqueue-wait lazyload" data-include="{{routes.root_url}}?section_id=section-iconbox"></div>
                {%- endif -%}
            {%- endif -%}
        </main>

        {% render 'mobile-footer' %}

        <footer>
          {%- section 'footer-widget' -%}
          {%- section 'footer' -%}
          {% comment %} {%- if request.design_mode -%}
              {%- if settings.enable_footerWidget -%}
              {%- section 'footer-widget' -%}{%- endif -%}
              {%- section 'footer' -%}
          {%- else -%}
              {%- if settings.enable_footerWidget -%}
              <div id="footerWidget" class="lazyqueue-wait lazyload" data-include="{{routes.root_url}}?section_id=footer-widget" data-lazyqueue></div>{%- endif -%}
              <div class="lazyqueue-wait lazyload" data-include="{{routes.root_url}}?section_id=footer" data-lazyqueue></div>
          {%- endif -%} {% endcomment %}
      </footer>


      </div>{%- capture content -%}{%- render 'modal' -%}{%- endcapture -%}{{header[1]}}

    {%- render 'compress', content: content -%}

    {%- if settings.review_app == 'loox' -%}{{ shop.metafields.loox["global_html_body"] }}{%- endif -%}
    <script>Loader.require('tiny', 'scripts');Loader.require('arn_theme', 'scripts');
        {%- if template == 'index'              -%}Loader.require('home', 'scripts');{%- endif -%}
        {%- if template contains 'collection'   -%}Loader.require('collection', 'scripts');{%- endif -%}
        {%- if template contains 'product'      -%}Loader.require('product', 'scripts');{%- endif -%}
        {%- if template contains 'page'         -%}Loader.require('page', 'scripts');{%- endif -%}
        {%- if template contains '404'          -%}Loader.require('page_404', 'scripts');{%- endif -%}
        {%- if settings.show_quickview          -%}Loader.require('quickview', 'scripts');{%- endif -%}
        {%- if template contains 'blog' or template contains 'article' -%}Loader.require('blog', 'scripts');{%- endif -%}
        window.addEventListener('DOMContentLoaded', function(){Loader.load();})</script>
