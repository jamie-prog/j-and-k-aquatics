<!doctype html>
<html lang="{{ shop.locale }}">
  <head>{% render 'pagefly-head' %}
    
    {%- capture content_for_head -%}
    <meta charset="utf-8">
    {%- if settings.favicon != nil -%}
    <link rel="shortcut icon" type="image/x-icon" href="{{ settings.favicon | img_url: '32x32' }}">
    <link rel="apple-touch-icon" sizes="32x32" href="{{ settings.favicon | img_url: '32x32' }}">
    {%- else -%}
    <link rel="shortcut icon" type="image/x-icon" href="{{ 'favicon.png' | asset_img_url: '32x32' }}">
    <link rel="apple-touch-icon" sizes="32x32" href="{{ 'favicon.png' | asset_img_url: '32x32' }}">
    {%- endif -%}
    <meta name="HandheldFriendly" content="True">
    <meta name="MobileOptimized" content="360">
    <meta name="viewport" content="width=device-width,initial-scale=1">
     <!--  PRELOAD DNS -->
    <link rel="preconnect dns-prefetch" href="https://cdn.shopify.com" crossorigin>
    <link rel="preconnect dns-prefetch" href="//fonts.googleapis.com" crossorigin>
    <link rel="dns-prefetch" href="https://fonts.gstatic.com" crossorigin>
    <link rel="dns-prefetch" href="https://monorail-edge.shopifysvc.com" crossorigin>
    <meta name="theme-color" content="{{ settings.main_color }}">
 	
    <!-- remove when live -->
    <meta name="robots" content="noindex">
    


	{% if customer %} 
        {% style %} .section-16206447478948b24c  {
        display: none;
    {% endstyle %}
    
    
    {% else %}
    {% style %} .section-16371633987102bd56 {
        display: none;
    {% endstyle %}
    {% endif %} 
    
    
    {% if customer %} 
        {% style %} .section-1595412396224 {
        display: none;
    {% endstyle %}
    
    
    {% else %}
    {% style %} .section-1637164301b7794f97 {
        display: none;
    {% endstyle %}
    {% endif %} 
    
    {% if customer %}
       {% style %} .hideprice {
          display: block;
      {% endstyle %}
      {% else %}
         {% style %} .hideprice {
          display: none;
      {% endstyle %}
    {% endif %}
    
    {% comment %}
 	{% if customer %}
        {% style %} .wsg-proxy-container {
            display: block;
        {% endstyle %}
        {% else %}
           {% style %} .wsg-proxy-container {
            display: none;
        {% endstyle %}
	{% endif %} 
    {% endcomment %}
    
    {% if customer %}
    	{% style %} #recentlyView .featured_product__item-body .product-price {
            display: inline-flex;
        {% endstyle %}
    {% else %}
    	{% style %} #recentlyView .featured_product__item-body .product-price {
            display: none;
        {% endstyle %}
    {% endif %} 
    
  

    <title>{{ page_title }}{%- if current_tags -%}{% assign meta_tags = current_tags | join: ', ' %} &ndash; {{ 'general.meta.tags' | t: tags: meta_tags }}{% endif %}{% if current_page != 1 %} &ndash; {{ 'general.meta.page' | t: page: current_page }}{% endif %}{% unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless %}</title>
    {%- if page_description -%}
    <meta name="description" content="{{ page_description | escape }}{% if current_tags %} {{ 'general.meta.tagged_html' | t: tags: meta_tags | escape }}.{% endif %}{% if current_page != 1 %} {{ 'general.meta.page' | t: page_number: current_page }}.{% endif %}" />
    {%- endif -%}

    {%- render 'snippet-theme', type: 'root' -%}
    {%- render 'theme-font' -%}
    <link rel="preload" href="{{ 'header.min.css' | asset_url }}" as="style">
    {%- if template == 'index' -%}<link rel="preload" href="{{ 'homepage.min.css'| asset_url }}" as="style">
    {%- elsif template contains 'collection' -%}<link rel="preload" href="{{ 'collection.min.css' | asset_url }}" as="style">
    {%- elsif template contains 'product' -%}<link rel="preload" href="{{ 'product.min.css' | asset_url }}" as="style">
    {%- elsif template contains 'cart' -%}<link rel="preload" href="{{ 'cart.min.css' | asset_url }}" as="style">
    {%- elsif template contains 'blog' or template contains 'article'   -%}<link rel="preload" href="{{ 'blog.min.css'       | asset_url }}" as="style">
    {%- else -%}<link rel="preload" href="{{ 'pages.min.css' | asset_url }}" as="style">
    {%- endif -%}

    {% if template contains 'collection' %}
    <link rel="canonical" href="{{ shop.url }}{{ collection.url }}" />
    {% else %}
    <link rel="canonical" href="{{ canonical_url }}" />
    {% endif %}

    <link href="{{ 'header.min.css' | asset_url }}" rel="stylesheet" type="text/css" media="all">
    {%- endcapture -%}
    {%- render 'compress', content: content_for_head -%}
    {%- render 'js-loader' -%}
    {%- render 'snippet-theme', type: 'script' -%}
    <script src="{{ 'lazysizes.min.js' | asset_url  }}" async="async"></script><script>document.addEventListener("click", x=>0)</script>

    {% include 'globo.filter.script' %}{{content_for_header}}

    {%- if settings.review_app == 'loox' -%}{{ shop.metafields.loox["global_html_head"] }}{%- endif -%}

