{%- liquid
    assign _pageLayout = settings.page_layout
-%}
<div class="page-404">
  {%- if _pageLayout == 'boxed' -%}<div class="container">{%- endif -%}
    <div class="page-content">
      <div class="row">
        <div class="col-lg-12 col-12">
          <div class="page-text-content">
            
            
            <img src="https://images.jnk-aquatics.co.uk/404.png" width="600"  alt=""/>

            <meta http-equiv="refresh" content="5; URL=https://ireland.jnk-aquatics.co.uk/">
            
            {% comment %}<h2>{{- 'general.404_page.subtitle' | t -}}</
            <br/> 
            <p>{{- 'general.404_page.text_redirect' | t -}}</p>{% endcomment %}
           <h3>It seems something fishy has happened. <br/><br/>
             Please wait whilst we re-direct you to the Home Page</h3><br/>
            <a href="{{ routes.root_url }}" class="btn btn-2">{{- 'general.404_page.button_redirect' | t -}}</a>
          </div>
        </div>
        <div class="col-lg-5 d-none d-lg-block">
      
        </div>
      </div>
    </div>
  
  
  {%- if _pageLayout == 'boxed' -%}</div>{%- endif -%}

  {%- section 'product-recommendation' -%}
</div>